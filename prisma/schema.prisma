generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model carnets {
  id Int @id @default(autoincrement())
  estado Boolean
  vendedores vendedores []
}

model vendedores {
  id Int @id @default(autoincrement())
  name String
  lastname String
  contrase√±a String
  carnet_id Int 
  carnet carnets @relation(fields: [carnet_id], references:[id])
}